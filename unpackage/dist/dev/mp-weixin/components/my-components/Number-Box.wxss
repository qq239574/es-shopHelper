.grace-number-box {
  width: 140rpx;
  height: 65rpx;
  display: -webkit-box !important;
  display: -webkit-flex !important;
  display: -ms-flexbox !important;
  display: flex !important;
  overflow: hidden;
  -webkit-flex-wrap: nowrap;
      -ms-flex-wrap: nowrap;
          flex-wrap: nowrap;
}
.grace-number-box .number {
    height: 100% !important;
    padding: 0 !important;
    width: 53rpx !important;
}
.grace-number-box .doBtn {
  width: 40rpx;
  line-height: 0;
  padding: 14rpx 0;
  font-weight: 700;
  height: 40rpx;
  text-align: center;
  font-size: 40rpx;
  color: #9fa5b0;
  border-radius: 50%;
  border: 1rpx solid #9fa5b0;
  margin: auto;
}
.grace-number-box .doBtn.primary {
    color: #fff;
    background: #fb6638;
    border: none;
    padding: 16rpx 0;
}
.grace-number-box input {
  display: block;
  width: 90rpx !important;
  height: 35rpx !important;
  line-height: 35rpx !important;
  color: #333;
  padding: 15rpx 0 !important;
  background: #F3F4F5;
  text-align: center !important;
}

