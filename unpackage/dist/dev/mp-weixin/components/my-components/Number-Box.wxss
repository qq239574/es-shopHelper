.grace-number-box {
  width: 140rpx;
  height: 65rpx;
  display: -webkit-box !important;
  display: -webkit-flex !important;
  display: -ms-flexbox !important;
  display: flex !important;
  overflow: hidden;
  -webkit-flex-wrap: nowrap;
      -ms-flex-wrap: nowrap;
          flex-wrap: nowrap;
}
.grace-number-box .number {
    height: 100% !important;
    padding: 0 !important;
    width: 53rpx !important;
}
.grace-number-box .doBtn {
  width: 42rpx;
  height: 42rpx;
  border-radius: 50%;
  border: 1rpx solid #9fa5b0;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  margin: auto;
  position: relative;
}
.grace-number-box .doBtn image {
    width: 30rpx;
    height: 30rpx;
    position: absolute;
    top: 5rpx;
    left: 5rpx;
}
.grace-number-box .doBtn.primary {
    background: #fb6638;
    border: 1rpx solid #fb6638;
}
.grace-number-box input {
  display: block;
  width: 90rpx !important;
  height: 35rpx !important;
  line-height: 35rpx !important;
  color: #333;
  padding: 15rpx 0 !important;
  background: #F3F4F5;
  text-align: center !important;
}

