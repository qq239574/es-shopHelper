<view class="provide-block" bind:__l="__l"><form data-event-opts="{{[['submit',[['formSubmit',['$event']]]]]}}" class="grace-form" bindsubmit="__e"><view class="grace-items"><view class="grace-label">商品类型</view><view class="address">{{goddType}}</view></view><view class="grace-items"><view class="grace-label">商品名称</view><view data-event-opts="{{[['tap',[['selectCell',[1,'$0'],['goodName']]]]]}}" class="other" catchtap="__e">{{''+goodName+''}}<text class="grace-icons icon-arrow-right" style="color:#5E5E5E;"></text></view></view><view class="grace-items"><view class="grace-label">副标题</view><view data-event-opts="{{[['tap',[['selectCell',[2,'$0'],['subTitle']]]]]}}" class="other" catchtap="__e">{{''+subTitle+''}}<text class="grace-icons icon-arrow-right" style="color:#5E5E5E;"></text></view></view><view class="grace-items"><view class="grace-label">商品分类</view><view data-event-opts="{{[['tap',[['selectCell',[3,'$0'],['classification']]]]]}}" class="other" catchtap="__e">{{''+classification+''}}<text class="grace-icons icon-arrow-right" style="color:#5E5E5E;"></text></view></view><view class="grace-items mainImage"><view class="grace-label " style="text-indent:1em;">主图</view><view class="imgbox"><choose-img imgTitle="主图" data-event-opts="{{[['^getImages',[['getImages']]]]}}" bind:getImages="__e"></choose-img></view></view><view class="grace-items swiper-imgs"><view class="grace-label" style="text-indent:1em;">轮播图</view><view class="imgbox"><choose-img maxNum="{{10}}" data-event-opts="{{[['^getImages',[['getImages']]]]}}" bind:getImages="__e"></choose-img><view class="tips">建议大小：750*750，长按图片可拖拽调整顺序</view></view></view></form><mpvue-picker class="vue-ref" themeColor="{{themeColor}}" pickerValueArray="{{list}}" pickerValueDefault="{{defaultVal}}" data-ref="mpvuePicker" data-event-opts="{{[['^onConfirm',[['onConfirm']]]]}}" bind:onConfirm="__e"></mpvue-picker></view>