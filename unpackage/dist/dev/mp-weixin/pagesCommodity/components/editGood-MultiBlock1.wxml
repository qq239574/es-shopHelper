<view class="provide-block" style="overflow:hidden;" bind:__l="__l"><input-item label="规格类型" other="{{info.goodType}}" value="{{info.goodType.value}}" disabled="{{true}}"></input-item><multi-block info="{{info.list.value}}" data-event-opts="{{[['^input',[['inputCell']]],['^click',[['clickCell']]]]}}" bind:input="__e" bind:click="__e"></multi-block><view class="white"><input-item label="划线价格" other="{{info.delPrice}}" value="{{info.delPrice.value}}" data-event-opts="{{[['^input',[['inputCell']]]]}}" bind:input="__e"></input-item><switch-item label="显示库存" other="{{info.showStock}}" checked="{{info.showStock.value}}" data-event-opts="{{[['^change',[['inputCell']]]]}}" bind:change="__e"></switch-item><input-item label="已出售数" other="{{info.soldNum}}" value="{{info.soldNum.value}}" data-event-opts="{{[['^input',[['inputCell']]]]}}" bind:input="__e"></input-item><switch-item label="显示销量" other="{{info.showSold}}" checked="{{info.showSold.value}}" data-event-opts="{{[['^change',[['inputCell']]]]}}" bind:change="__e"></switch-item></view></view>