.provide-block {
  background: #fff;
  margin-top: 20rpx;
  overflow: auto;
}
.provide-block .grace-items {
    width: 710rpx;
    margin: auto;
    padding: 2rpx auto;
}
.provide-block .grace-items .address {
      height: -webkit-fit-content;
      height: -moz-fit-content;
      height: fit-content;
      width: 490rpx;
      line-height: 38rpx;
}
.provide-block .grace-items .imgbox {
      width: 490rpx;
      padding-bottom: 10rpx;
}
.provide-block .grace-items .imgbox .tips {
        width: 100%;
        font-size: 20rpx;
        text-align: left;
        color: #9ea3ae;
        line-height: 60rpx;
}
.provide-block .grace-items.mainImage {
      height: 200rpx;
      padding-top: 0;
}
.provide-block .grace-items.swiper-imgs {
      min-height: 200rpx;
      padding: 0 0 10rpx;
}

