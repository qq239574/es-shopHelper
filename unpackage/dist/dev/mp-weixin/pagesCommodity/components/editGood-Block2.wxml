<view class="provide-block" bind:__l="__l"><form data-event-opts="{{[['submit',[['formSubmit',['$event']]]]]}}" class="grace-form" bindsubmit="__e"><select-item label="规格类型" other="{{info.specification}}" value="{{info.specification.value}}" data-event-opts="{{[['^click',[['clickCell',['$event']]]]]}}" catch:click="__e"></select-item><block wx:if="{{info.specification.value=='多规格'}}"><select-item label="子规格详情" other="{{info.childrenSpecs}}" value="{{info.childrenSpecs.value}}" data-event-opts="{{[['^click',[['clickCell',['$event']]]]]}}" catch:click="__e"></select-item></block><block wx:if="{{!info.price.needHide}}"><input-item label="售卖价格" type="digit" other="{{info.price}}" value="{{info.price.value}}" data-event-opts="{{[['^input',[['inputCell']]]]}}" bind:input="__e"></input-item></block><input-item label="划线价格" type="digit" other="{{info.delPrice}}" value="{{info.delPrice.value}}" data-event-opts="{{[['^input',[['inputCell']]]]}}" bind:input="__e"></input-item><block wx:if="{{!info.cardStock.needHide}}"><select-item label="卡密库" other="{{info.cardStock}}" value="{{info.cardStock.value}}" data-event-opts="{{[['^click',[['clickCell',['$event']]]]]}}" catch:click="__e"></select-item></block><block wx:if="{{!info.stockNum.needHide&&info.cardStock.needHide}}"><input-item label="商品库存" type="number" other="{{info.stockNum}}" value="{{info.stockNum.value}}" data-event-opts="{{[['^input',[['inputCell']]]]}}" bind:input="__e"></input-item></block><switch-item label="显示库存" other="{{info.showStock}}" checked="{{info.showStock.value}}" data-event-opts="{{[['^change',[['inputCell']]]]}}" bind:change="__e"></switch-item><input-item label="已出售数" type="number" other="{{info.soldNum}}" value="{{info.soldNum.value}}" data-event-opts="{{[['^input',[['inputCell']]]]}}" bind:input="__e"></input-item><switch-item label="显示销量" other="{{info.showSold}}" checked="{{info.showSold.value}}" data-event-opts="{{[['^change',[['inputCell']]]]}}" bind:change="__e"></switch-item></form><mpvue-picker class="vue-ref" themeColor="{{themeColor}}" pickerValueArray="{{list}}" pickerValueDefault="{{defaultVal}}" data-ref="mpvuePicker" data-event-opts="{{[['^onConfirm',[['onConfirm']]]]}}" bind:onConfirm="__e"></mpvue-picker></view>