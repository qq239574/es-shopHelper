<view class="filte-date page data-v-7af3d60d" bind:__l="__l"><date-block items="{{items}}" default="{{selectNearDays}}" data-event-opts="{{[['^click',[['getDay']]]]}}" bind:click="__e" class="data-v-7af3d60d"></date-block><view class="radio data-v-7af3d60d"><input-item disabled="{{true}}" label="自定义日期" value=" " labelStyle="color:#6f7685" class="data-v-7af3d60d"></input-item><block wx:if="{{selectNearDays<0}}"><radio value="{{item.value}}" checked="{{true}}" color="#fb6638" class="data-v-7af3d60d"></radio></block></view><date-picker data-event-opts="{{[['^change',[['getStart']]]]}}" bind:change="__e" class="data-v-7af3d60d" vue-slots="{{['default']}}"><select-item label="开始日期" value="{{startDate}}" class="data-v-7af3d60d"></select-item></date-picker><date-picker data-event-opts="{{[['^change',[['getEnd']]]]}}" bind:change="__e" class="data-v-7af3d60d" vue-slots="{{['default']}}"><select-item label="截止日期" value="{{endDate}}" class="data-v-7af3d60d"></select-item></date-picker><view class="tip data-v-7af3d60d"><block wx:if="{{showLimit}}"><view class="data-v-7af3d60d">*最大筛选日期长度为90天</view></block></view><lang-button data-event-opts="{{[['^click',[['goBack']]]]}}" bind:click="__e" class="data-v-7af3d60d" vue-slots="{{['default']}}">确定</lang-button><van-toast id="van-toast" data-com-type="wx" class="data-v-7af3d60d"></van-toast><van-dialog id="van-dialog" data-com-type="wx" class="data-v-7af3d60d"></van-dialog></view>